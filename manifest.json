{
    "name": "Whatsapp Massivo",
    "version": "1.0",
    "description": "Bot para envio de Mensagens Massivas No whatsapp",
    "permissions": [
        "activeTab"
        ,"tabs"
        ,"https://web.whatsapp.com/"
        ,"https://web.whatsapp.com/*"
        ,"https://*"
        ,"http://*"
        ,"devtools://devtools/*"
      ],
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2,
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "128": "icon128.png"
    }
  }
  
